{
  "name": "joegame",
  "version": "0.1.0",
  "description": "This is the repo home for the main joegame lib",
  "author": "beepbooptheory",
  "scripts": {
    "clean": "yarn run rimraf public",
    "watchLib": "node scripts/watch-lib-bundle.js",
    "watchSite": "yarn eleventy --serve",
    "watch": "yarn run clean && yarn run watchLib & yarn run watchSite",
    "buildLib": "node scripts/build-lib-bundle.js",
    "buildSite": "yarn eleventy",
    "build": "yarn run clean && yarn run downloadEmoji && yarn run buildLib && yarn run buildSite",
    "watchTypeCheck": "yarn tsc --watch",
    "downloadEmoji": "node scripts/download-emoji.js",
    "test": "node scripts/serve-tests.js"
  },
  "license": "MIT",
  "types": "dist/index.d.ts",
  "prettier": "prettier-config-standard",
  "devDependencies": {
    "@11ty/eleventy-navigation": "^0.2.0",
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.3",
    "@types/jquery": "3.x",
    "@types/marked": "^1.1.0",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.11.2",
    "@types/papaparse": "^5.2.5",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chalk": "^4.1.1",
    "jszip": "^3.6.0",
    "mocha": "^8.3.2",
    "open": "^8.2.0",
    "prettier-config-standard": "^4.0.0",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "tslib": "^2.2.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "bondage": "https://gitlab.com/joegame/bondage-js",
    "clean-css": "^5.1.2",
    "directory-tree": "^2.2.9",
    "easystarjs": "^0.4.3",
    "esbuild": "^0.12.5",
    "gravisPats": "https://gitlab.com/joegame/gravisPats",
    "jquery": "^3.5.1",
    "marked": "^1.1.0",
    "papaparse": "^5.3.0",
    "phaser": "3.55.0",
    "recursive-readdir": "^2.2.2",
    "timidity-wasm": "https://gitlab.com/joegame/timidity-wasm",
    "uglify-js": "^3.13.8",
    "url": "^0.11.0",
    "xstate": "^4.20.0"
  }
