{
  "name": "joegame-lib",
  "version": "0.8.0",
  "description": "A typescript library for high level, functional Phaser 3 control and game creation, featuring bondage, easystarjs, xstate",
  "author": "beepbooptheory",
  "main": "dist/index.js",
  "scripts": {
    "clean": "yarn run rimraf dist",
    "build:js": "yarn run clean && babel --source-maps -d dist -x \".ts,.js\" src",
    "build:def": "tsc --emitDeclarationOnly",
    "watch:js": "yarn run clean && babel --source-maps -d dist -x \".ts,.js\" -w src",
    "watch:def": "tsc --emitDeclarationOnly --watch",
    "build": "yarn run build:js & yarn run build:def",
    "watch": "yarn run watch:js & yarn run watch:def",
    "test": "yarn webpack serve -c webpack.test.js"
  },
  "license": "MIT",
  "types": "dist/index.d.ts",
  "prettier": "prettier-config-standard",
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/plugin-transform-typescript": "^7.5.5",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.3",
    "@types/jquery": "3.x",
    "@types/lodash": "^4.14.168",
    "@types/marked": "^1.1.0",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.11.2",
    "@types/papaparse": "^5.2.5",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "copy-webpack-plugin": "^8.1.1",
    "html-webpack-plugin": "^5.3.1",
    "mocha": "^8.3.2",
    "prettier-config-standard": "^4.0.0",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "source-map-loader": "^2.0.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.2",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "bondage": "https://gitlab.com/beepbooptheory/bondage-js",
    "easystarjs": "^0.4.3",
    "jquery": "^3.5.1",
    "lodash": "^4.17.21",
    "marked": "^1.1.0",
    "papaparse": "^5.3.0",
    "phaser": "3.54.0",
    "tone": "^14.8.15",
    "url": "^0.11.0",
    "xstate": "^4.13.0"
  }
}
