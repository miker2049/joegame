{
  "name": "bondage",
  "version": "2.2.1",
  "description": "Yarn parser for Javascript",
  "main": "dist/bondage.min.js",
  "scripts": {
    "test": "NODE_ENV=node node node_modules/mocha/bin/mocha tests --recursive",
    "lint": "node_modules/.bin/eslint .",
    "build": "yarn run compileParser && yarn run bundle",
    "bundle": "esbuild --define:process.env.NODE_ENV=\\\"browser\\\" --bundle --minify --platform=browser --target=es6 src/bondage.js --outfile=dist/bondage.min.js",
    "compileParser": "jison src/parser/grammar.json -o src/parser/parser-browser.js -m js && jison src/parser/grammar.json -o src/parser/parser-node.js -m commonjs"
  },
  "types": "bondage.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhayley/bondage.js.git"
  },
  "author": "j hayley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jhayley/bondage.js/issues"
  },
  "homepage": "https://github.com/jhayley/bondage.js#readme",
  "devDependencies": {
    "chai": "^4.0.1",
    "esbuild": "^0.11.14",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "mocha": "^3.4.2",
    "jison": "^0.4.18"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "inquirer": "^3.0.6"
  }
}
